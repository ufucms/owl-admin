{
    "name": "ufucms/ufu-admin",
    "description": "采用 laravel、amis 打造的后台框架，拥有 120+ 种组件，只用后端代码就能实现复杂的页面效果，让开发者能够快速构建高效的后台管理系统。",
    "license": "MIT",
    "authors": [
        {
            "name": "Slowlyo",
            "email": "slowlyo_email@qq.com.com",
            "homepage": "https://gitee.com/slowlyo"
        }
    ],
    "homepage": "https://gitee.com/slowlyo/owl-admin",
    "keywords": [
        "Laravel",
        "OwlAdmin",
        "Admin",
        "Amis"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "~9|~10",
        "stancl/virtualcolumn": "^1.3",
        "symfony/yaml": "^3.4|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Slowlyo\\OwlAdmin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slowlyo\\OwlAdmin\\AdminServiceProvider"
            ],
            "aliases": {
                "OwlAdmin": "Slowlyo\\OwlAdmin\\Facades\\OwlAdmin"
            }
        }
    }
}
